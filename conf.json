{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.jsx?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "opts": {
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "template": "./node_modules/better-docs"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "better-docs": {
      "name": "Scran | A Recipe App for the Web",
      "logo": "../assets/scran-logo.png",
      "title": "Scran Documentation"
    }
  }
}
